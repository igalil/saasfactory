{
  "name": "saasfactory",
  "version": "0.1.0",
  "description": "CLI tool to generate production-ready SaaS boilerplates using Claude AI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "saasfactory": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "bun dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "prepublishOnly": "bun run build && bun test -- --run"
  },
  "keywords": [
    "saas",
    "boilerplate",
    "generator",
    "nextjs",
    "convex",
    "clerk",
    "stripe",
    "cli",
    "ai",
    "claude",
    "typescript",
    "tailwind",
    "shadcn"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/saasfactory.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/saasfactory/issues"
  },
  "homepage": "https://github.com/your-username/saasfactory#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  }
}
